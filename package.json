{
    "name": "seller-kanrikun",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "scripts": {
        "dev": "remix vite:dev",
        "build": "remix vite:build",
        "start": "wrangler pages dev",
        "deploy": "bun run build && wrangler pages deploy",
        "typecheck": "tsc",
        "cf-typegen": "wrangler types",
