        "dotenv-cli": "^7.4.2",
        "drizzle-kit": "^0.27.1",
        "postcss": "^8.4.47",
        "tailwindcss": "^3.4.14",
        "typescript": "^5.6.3",
        "vite": "^5.4.10",
        "vite-tsconfig-paths": "^5.1.0",
        "wrangler": "3.84.1"
